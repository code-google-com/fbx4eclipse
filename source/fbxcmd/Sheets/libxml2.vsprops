<?xml version="1.0"?>
<VisualStudioPropertySheet 
	ProjectType="Visual C++" 
	Version="8.00" 
	Name="libxml2"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(LIBXML2DIR)include\"
		PreprocessorDefinitions=""
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="libxml2_a.lib"
		AdditionalLibraryDirectories="$(LIBXML2DIR)lib\"
	/>
	<UserMacro Name="LIBXML2DIR" Value="$(SolutionDir)Libraries\libxml2-2.7.6\" /> 
</VisualStudioPropertySheet>
